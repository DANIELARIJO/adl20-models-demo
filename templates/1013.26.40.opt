            </term_definitions>
            <term_definitions code="at0005">
              <items id="text">Clear</items>
              <items id="description">No inflammatory signs of atopic dermatitis.</items>
            </term_definitions>
            <term_definitions code="at0006">
              <items id="text">Almost clear</items>
              <items id="description">Just perceptible erythema and just perceptible papulation/infiltration.</items>
            </term_definitions>
            <term_definitions code="at0007">
              <items id="text">Mild disease</items>
              <items id="description">Mild erythema and mild papulation/infiltration.</items>
            </term_definitions>
            <term_definitions code="at0008">
              <items id="text">Moderate disease</items>
              <items id="description">Moderate erythema and moderate papulation/infiltration.</items>
            </term_definitions>
            <term_definitions code="at0009">
              <items id="text">Severe disease</items>
              <items id="description">Severe erythema and severe papulation/infiltration.</items>
            </term_definitions>
            <term_definitions code="at0010">
              <items id="text">Very severe disease</items>
              <items id="description">Severe erythema and severe papulation/infiltration with oozing/crusting.</items>
            </term_definitions>
